<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DownloadController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXButton fx:id="btnDownload" buttonType="RAISED" onAction="#downloadOnClick" stylesheets="@Styles.css" text="Create new CSV File">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></JFXButton>
            <JFXButton fx:id="btnOverwrite" buttonType="RAISED" onAction="#overwriteOnClick" stylesheets="@Styles.css" text="Overwrite CSV File" />
         </children>
      </VBox>
   </children>
</AnchorPane>
